        <activity android:name=".ajoutAgentAdmin" />
        <activity android:name=".dashboardClient" />
        <activity android:name=".dashboardAgent" />
        <activity android:name=".dashboardAdmin" />
        <activity android:name=".login" />
        <activity android:name=".creationCompte" />
        <activity android:name=".devise" />
        <activity android:name=".NewClient" />
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Currency_Activity"></activity>
    </application>

</manifest>